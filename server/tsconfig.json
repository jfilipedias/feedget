{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "types": [
      "node",
    ],
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}